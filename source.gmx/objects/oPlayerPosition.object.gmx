    align = oPlayer.x mod 3
    ypos = oPlayer.y
    xpos = oPlayer.x
}

var yy = y+5
var dy = 20
var xx = x+7
draw_text(xx,yy,'Align    '+string(align))
yy += dy
draw_text(xx,yy,'X pos   '+string(xpos))
yy += dy
var ystr = string(ypos)
if global.playery_extended ystr = string_format(ypos,3,16)
draw_text(xx,yy,'Y pos   '+ystr)

var buttons_active = global.state == globalstate_idle and not global.comboboxselected
var buttonw = 24
var buttonh = 12
var buttonx = x+136-buttonw
var buttony = y
if menuButton(buttonx,buttony,buttonw,buttonh,'',buttons_active,false) {
    global.playery_extended = not global.playery_extended
    saveConfig()
}
fontSetup(font_small,c_black,fa_center,fa_center,1)
