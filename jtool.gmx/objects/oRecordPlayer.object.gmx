            <kind>1</kind>
            <string>///walljumps

var onVineL = distance_to_object(oWalljumpL) &lt;= 1 and place_free(x,y+grav)
var onVineR = distance_to_object(oWalljumpR) &lt;= 1 and place_free(x,y+grav)

if onVineL or onVineR {
    if onVineR xscale = -1
    else xscale = 1
    vspeed = 2*grav
    sprite_index = sPlayerSliding
    image_speed = 0.5
    
    // pressed away from the vine
    if (onVineL and PR || FP)
    or (onVineR and PL || FO) {
        
        // jumping off vine
        if J {
            if onVineR hspeed = -15
            else hspeed = 15
            vspeed = -9 * grav
            audio_play_sound(sndWallJump,0,0)
            sprite_index = sPlayerJump
        }
        
        // walking off vine
        else {
            if onVineR hspeed = -3
            else hspeed = 3
            sprite_index = sPlayerFall
        }
    }
}
</string>
